package corejava;

public class incrementdecerement {
public static void main(String[] args) {
	
	//increment
	int a=10;
	System.out.println(++a);
	int b=15;
	System.out.println(b++);
	System.out.println(b++);
	
	//decrement
	int c=25;
	System.out.println(--c);
	int d=35;
	System.out.println(d--);
	System.out.println();
}
}
